# Environment variables for Z-Image Web
# Copy this file to .env.local and update the values

# API Base URL for Z-Image backend service
VITE_API_BASE_URL=http://localhost:8000

# Alternative URLs for different environments
# VITE_API_BASE_URL=https://your-production-api.com
# VITE_API_BASE_URL=https://sandbox-session7e4863e5a65643f7b2-8000.preview.iflow.cn

# Feature flags
VITE_ENABLE_EDIT_FEATURE=true
VITE_ENABLE_GALLERY_FEATURE=true

# Analytics (optional)
# VITE_ANALYTICS_ID=your-analytics-id